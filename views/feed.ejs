<%- include('partials/header') -%>

<div>
    <div>
        <div class="feed-header">
            <div>
                <img src="/img/logo.svg" alt="">
            </div>
            <h1>
                <a href="/profile"><%= user.userName %>'s Profile</a>
            </h1>
        </div>
        <ul class="parent">
            <% for(let i = 0; i < posts.length; i++) {%>
                <li>
                    <a href="/post/<%= posts[i]._id%>">
                        <img class="child" src="<%= posts[i].image%>" alt="">
                    </a>
                </li>
            <% } %>
        </ul>
    </div>
</div>

<footer class="index-header">
    <nav class="index-nav">
        <div>
            <img src="/img/logo.svg" alt="">
        </div>
        <div>
            <a href="/profile" class="btn btn-login">Return to Profile</a>
            <a href="/feed" class="btn btn-login">Return to Feed</a>
        </div>
    </nav>
</footer>

<%- include('partials/footer') -%>